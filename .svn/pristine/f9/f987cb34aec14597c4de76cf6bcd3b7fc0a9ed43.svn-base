.vertical-box {
    display: table;
    table-layout: fixed;
    border-spacing: 0;
    height: 100%;
    width: 100%;
}

.vertical-box-column {
    display: table-cell;
    vertical-align: top;
    height: 100%;
}

.vertical-box-row {
    display: table-row;
    height: 100%;
}

    .vertical-box-row > .vertical-box-cell {
        position: relative;
        height: 100%;
        width: 100%;
        float: none;
    }

        .vertical-box-row > .vertical-box-cell > .vertical-box-inner-cell {
            position: absolute;
            top: 0;
            bottom: 0;
            left: 0;
            right: 0;
            overflow: hidden;
        }

.panel-expand .vertical-box .vertical-box-column {
    display: table-cell;
}

